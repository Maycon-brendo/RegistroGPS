<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph.xml"
    app:startDestination="@id/homeFragment2">

    <fragment
        android:id="@+id/homeFragment2"
        android:name="com.example.registrogps.fragments.HomeFragment"
        android:label="HomeFragment" >
        <action
            android:id="@+id/action_homeFragment2_to_listaRegistroFragment2"
            app:destination="@id/listaRegistroFragment2" />
        <action
            android:id="@+id/action_homeFragment2_to_configuracaoFragment2"
            app:destination="@id/configuracaoFragment2" />
    </fragment>
    <fragment
        android:id="@+id/listaRegistroFragment2"
        android:name="com.example.registrogps.fragments.ListaRegistroFragment"
        android:label="fragment_lista_registro"
        tools:layout="@layout/fragment_lista_registro" />
    <fragment
        android:id="@+id/configuracaoFragment2"
        android:name="com.example.registrogps.fragments.ConfiguracaoFragment"
        android:label="fragment_configuracao"
        tools:layout="@layout/fragment_configuracao" />
</navigation>